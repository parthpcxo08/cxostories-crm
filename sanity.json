{
  "root": true,
  "project": {
    "name": "cxostories_crm"
  },
  "api": {
    "projectId": "gh3tmbwn",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/dashboard",
    "@sanity/desk-tool",
    "dashboard-widget-structure-menu",
    "tags",
    "markdown",
    "dashboard-widget-document-list"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision",
        "dashboard-widget-netlify"
      ]
    }
  },
  "parts": [
    {
      "implements": "part:@sanity/dashboard/config",
